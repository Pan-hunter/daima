#include <stdio.h>
#include <stdlib.h>

int main()
{   int a, b, c;
    int t;
    printf("请输入三个整数:");
    scanf("%d %d %d",&a , &b, &c);
    if (a<b)
    {
        t=a;
        a=b;
        b=t;
    }

    if(a<c)
    {
        t=a;
        a=c;
        c=t;
    }

    if(b<c)

    {
        t=b;
        b=c;
        c=t;
    }
    printf("%d %d %d\n", a, b, c);
    return 0;
}
//用第一个和后面的依次来比较，再接着用第二个（三个常数时）
